      <Separator layoutY="61.0" prefHeight="17.0" prefWidth="520.0" />
      <ImageView fitHeight="128.0" fitWidth="196.0" layoutX="162.0" layoutY="89.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@news-paper-pdf.png" />
         </image>
      </ImageView>
      <Label layoutX="87.0" layoutY="275.0" text="PaperName:">
         <font>
            <Font name="Constantia Bold Italic" size="16.0" />
         </font></Label>
      <Label layoutX="87.0" layoutY="329.0" text="Price:">
         <font>
            <Font name="Constantia Bold Italic" size="16.0" />
         </font></Label>
      <ComboBox fx:id="combotitle" editable="true" layoutX="193.0" layoutY="271.0" prefWidth="150.0" />
      <TextField fx:id="txtprice" layoutX="192.0" layoutY="325.0" />
      <Button layoutX="30.0" layoutY="415.0" mnemonicParsing="false" onAction="#donew" text="NEW">
