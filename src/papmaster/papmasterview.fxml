<AnchorPane minHeight="520.0" minWidth="520.0" style="-fx-background-color: lightgreen;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="papmaster.papmasterviewController">
   <children>
      <Label alignment="CENTER" layoutX="115.0" layoutY="14.0" prefHeight="43.0" prefWidth="275.0" text="-_- PAPER MASTER -_-" textFill="#943232">
