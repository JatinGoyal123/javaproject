<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="500.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="listbox.listboxviewController">
   <children>
      <ComboBox fx:id="txtcombo" layoutX="176.0" layoutY="53.0" onAction="#doselect" prefWidth="150.0" />
      <ListView fx:id="txtlist" layoutX="168.0" layoutY="124.0" prefHeight="167.0" prefWidth="165.0" />
      <Label layoutX="168.0" layoutY="361.0" prefHeight="17.0" prefWidth="150.0" />
      <Button fx:id="btnshow" layoutX="385.0" layoutY="130.0" mnemonicParsing="false" onAction="#doshow" text="Show All" />
      <Button fx:id="btnclear" layoutX="386.0" layoutY="195.0" mnemonicParsing="false" onAction="#doclear" text="Clear All" />
      <Button fx:id="btndelete" layoutX="382.0" layoutY="266.0" mnemonicParsing="false" onAction="#dodelete" text="Delete All" />
      <Label fx:id="txtlabel" layoutX="181.0" layoutY="370.0" prefHeight="17.0" prefWidth="140.0" text="Label" />
   </children>
</AnchorPane>
